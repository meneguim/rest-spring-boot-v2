package br.com.erudio;

import br.com.erudio.exception.UnsupportedMathOperationException;

public class Calc {
	
	public static Double sum (Double n1, Double n2) {
		if (n1 == null || n2 == null) {
			throw new UnsupportedMathOperationException("Na soma o primeiro e segundo numero devem ser especificados"); 
		}
		return n1 + n2;
	}
	
}
